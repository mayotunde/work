  <div id="map" style='width:700px;height:500px'></div>
  <script>
 var map = L.map('map').setView([6.44, 3.55], 7);
 mapLink = '<a href="http://openstreetmap.org">openstreetmap</a>,'+'<a href="http://mapsnigeriainitiative.wordpress.com">mapsNI</a>,'+'Census data:NPC 2006'; 
  var tile = L.tileLayer('http://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {attribution: ' &copy; ' + mapLink,maxZoom: 18}).addTo(map);
  
  var jma = "https://cdn.rawgit.com/mayotunde/work/gh-pages/districtLagos.json?callback=?";
  $.getJSON(jma, function(data) {
  L.geoJson(data).addTo(map);
  });
      
  </script>
